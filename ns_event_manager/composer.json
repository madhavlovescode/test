{
    "name": "nitsan/ns-event-manager",
    "type": "typo3-cms-extension",
    "description": "here i have created an extension nammed EventCrud.",
    "authors": [],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^11.5"
    },
    "require-dev": {
        "typo3/testing-framework": "^6.9.0"
    },
    "autoload": {
        "psr-4": {
            "NITSAN\\NsEventManager\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NITSAN\\NsEventManager\\Tests\\": "Tests"
        }
    },
    "replace": {
        "typo3-ter/ns-event-manager": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "scripts": {
        "post-autoload-dump": [
            "TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
        ]
    },
    "extra": {
        "typo3/cms": {
            "web-dir": ".Build/public",
            "extension-key": "ns_event_manager"
        }
    }
}